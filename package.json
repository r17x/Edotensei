{
  "name": "edotensei",
  "version": "1.0.3",
  "description": "Simple load Resource/Assets on the browser.",
  "main": "dist/edotensei.js",
  "module": "dist/edotensei.es.js",
  "files": [
    "src",
    "dist"
  ],
  "repository": "git@github.com:ri7nz/Edotensei.git",
  "author": "ri7nz <ri7nz.labs@gmail.com>",
  "license": "MIT",
  "private": false,
  "bundlesize": [
    {
      "path": "./dist/*.js",
      "maxSize": "2 kB"
    }
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "bundlesize": "^0.17.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.7.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-jest": "^21.26.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^1.1.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.1"
  },
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:cjs": "babel src --out-dir dist --source-maps --ignore tests",
    "build:es": "cross-env BABEL_ENV=es babel src --out-file dist/edotensei.es.js --source-maps --ignore tests",
    "build:umd": "cross-env BABEL_ENV=umd rollup -c",
    "build:umd-min": "cross-env BABEL_ENV=umd NODE_ENV=production rollup -c",
    "build": "npm-run-all --silent build:*",
    "lint": "eslint -c .eslintrc ./src --fix",
    "prepare": "rm -rf dist && yarn build",
    "test": "jest --config jest.config.js",
    "test-watch": "jest --watch",
    "test-size": "bundlesize"
  }
}
